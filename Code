    //checks if input (ans) is correct using column number (random1) and row number (random2) in soup method
    //returns true if correct, false if not
    public boolean ValidSoup (int random1, int random2, int ans)
    {
        if (random2 == 1 && ans == (Math.pow (4, (random1 - 1))))
            return true;
        else if (random2 == 2 && ans == ((Math.pow (4, (random1 - 1))) * 5))
            return true;
        else if (random2 == 3 && ans == ((Math.pow (4, (random1 - 1))) * 25))
            return true;
        else if (random2 == 4 && ans == (Math.pow (4, (random1 - 1)) * 125))
            return true;
        else if (random2 == 5 && ans == (Math.pow (4, (random1 - 1)) * 625))
            return true;
        else if (random2 == 6 && ans == (Math.pow (4, (random1 - 1)) * 3125))
            return true;
        else
            return false;

    }


    //user picked soup from menu (option method)
    //asks to name numbers
    //asks for a integer (int variable)
    //keeps track of score for this section (therefore score is reset) and returns it at the end (as an integer)
    public int soup (int score)
    {
        score = 0;

        System.out.println ("\n\t\t\t ***You picked***");
        soupName ();

        System.out.println ("\n\nYou don't have to be 'soup-erman' (or 'soup-erwoman') to complete this round...\n\n\n");

        multiply ();
        int digit = 2;

        //asks the same overall question, but with random numbers to change it up
        //user names numbers in table from multiply mathod
        for (int i = 1 ; i <= 5 ; i++)
        {

            int random1 = (int) (Math.random () * 7) + 1;
            int random2 = (int) (Math.random () * 6) + 1;
            int ans = IBIO.inputInt ("\nWhat is the number in column " + random1 + ", row " + random2 + "?\n");
            if (ValidSoup (random1, random2, ans))
            {
                score++;
                System.out.println ("\nThat's right. \nYou gained a point. Your total score for soup is: " + score);
            }
            else
            {
                System.out.println ("\nThat's wrong, try another question.");
                score--;
                System.out.println ("You lost a point. Your total score for soup is now: " + score);
            }


        }

        //prints prompts according to score
        if (score > 4)
            printSlow ("\nYou did very well!");
        if (score > 2)
        {
            printSlow ("\nYou have gained enough points to receive soup!");
            //prints picture of soup
            souppic ();
        }


        else
            System.out.println ("\nSorry, you have not gained enough points to receive soup! >_<\n\n\n\n");
        pause ();
        return score;

    }


    //user picked hot drinks from menu (option method)
    //asks questions about coffee
    //asks for a word (String variable)
    //keeps track of score for this section (therefore score is reset) and returns it at the end (as an integer)
    public int hotDrink (int score)
    {
        //score is increased by 4 with every correct answer
        //user still awarded points based on their answers' likeness to the correct answer.

        score = 0;

        System.out.println ("\n\t\t\t\t\t***You picked***");
        hotDrinkName ();

        System.out.println ("\n\n'Java' moment to finish this quiz?\nThanks 'a latte' for not thinking it's too silly!\n");

        System.out.println ("The following are all multiple choice questions\nYour answer should be the according letter.\nThe correct answer will be awarded the full 4 points.\nIncorrect answers may be awarded points based on their likeness to the correct answer.\n");

        char ans1 = IBIO.inputChar ("\nCoffee is the world's ___ most legally traded commodity. \na)\t1st\nb)\t2nd\nc)\t7th\nd)\t11th\n");
        if (ans1 == ('b') || ans1 == ('B'))
        {
            score += 4;
            System.out.println ("\nThat's right. \nYou gained 4 points. Your score is now: " + score);
        }

        else if (ans1 == ('a') || ans1 == ('A'))
        {
            score += 3;
            System.out.println ("\nThat's wrong. \nYou still gained 3 points because you answer was close. Your score is now: " + score);
            System.out.println ("The correct answer was 'b) 2nd'.");
            System.out.println ("\nTry another question.");
        }

        else if (ans1 == ('c') || ans1 == ('C'))
        {
            score += 2;
            System.out.println ("\nThat's wrong. \nYou still gained 2 points because you answer wasn't absurd. Your score is now: " + score);
            System.out.println ("The correct answer was 'b) 2nd'.");
            System.out.println ("\nTry another question.");
        }

        else if (ans1 == ('d') || ans1 == ('D'))
        {
            score += 1;
            System.out.println ("\nThat's wrong. \nYou still gained 1 points because you answer was an option. Your score is now: " + score);
            System.out.println ("The correct answer was 'b) 2nd'.");
            System.out.println ("\nTry another question.");
        }

        else
        {
            //Show an error dialog that displays the message, 'That's NOT EVEN AN OPTION!':
            JOptionPane.showMessageDialog (null, "That's NOT EVEN AN OPTION!", "Alert",
                    JOptionPane.ERROR_MESSAGE);
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was 'b) 2nd'.");
            System.out.println ("\nTry another question.");
        }


        char ans2 = IBIO.inputChar ("\nCoffee is sometimes known as a ___ of ___. \na)\tmug, caff\nb)\tshot, brewery\nc)\tglass, cap\nd)\tcup, joe\n");
        if (ans2 == ('d') || ans2 == ('D'))
        {
            score += 4;
            System.out.println ("\nThat's right. \nYou gained 4 points. Your score is now: " + score);
        }

        else if (ans2 == ('a') || ans2 == ('A'))
        {
            score += 3;
            System.out.println ("\nThat's wrong. \nYou still gained 3 points because you answer was close. Your score is now: " + score);
            System.out.println ("The correct answer was 'd) cup, joe'.");
            System.out.println ("\nTry another question.");
        }

        else if (ans2 == ('c') || ans2 == ('C'))
        {
            score += 2;
            System.out.println ("\nThat's wrong. \nYou still gained 2 points because you answer wasn't absurd. Your score is now: " + score);
            System.out.println ("The correct answer was 'd) cup, joe'.");
            System.out.println ("\nTry another question.");
        }

        else if (ans2 == ('b') || ans2 == ('B'))
        {
            score += 1;
            System.out.println ("\nThat's wrong. \nYou still gained 1 points because you answer was an option. Your score is now: " + score);
            System.out.println ("The correct answer was 'd) cup, joe'.");
            System.out.println ("\nTry another question.");
        }

        else
        {
            JOptionPane.showMessageDialog (null, "That's NOT EVEN AN OPTION!", "Alert",
                    JOptionPane.ERROR_MESSAGE);
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was 'd) cup, joe'.");
            System.out.println ("\nTry another question.");
        }


        char ans3 = IBIO.inputChar ("\n___ is the Italian word for milk. So if you request a ____ in Italy, you'll be served a glass of milk. \na)\tespresso\nb)\tlatte\nc)\tcreme\nd)\tcaffe\n\n");
        if (ans3 == ('b') || ans3 == ('B'))
        {
            score += 4;
            System.out.println ("\nThat's right. \nYou gained 4 points. Your score is now: " + score);
        }

        else if (ans3 == ('c') || ans3 == ('C'))
        {
            score += 3;
            System.out.println ("\nThat's wrong. \nYou still gained 3 points because you answer was close. Your score is now: " + score);
            System.out.println ("The correct answer was 'b) latte'.");
            System.out.println ("\nTry another question.");
        }

        else if (ans3 == ('a') || ans3 == ('A'))
        {
            score += 2;
            System.out.println ("\nThat's wrong. \nYou still gained 2 points because you answer wasn't absurd. Your score is now: " + score);
            System.out.println ("The correct answer was 'b) latte'.");
            System.out.println ("\nTry another question.");
        }

        else if (ans3 == ('d') || ans3 == ('D'))
        {
            score += 1;
            System.out.println ("\nThat's wrong. \nYou still gained 1 points because you answer was an option. Your score is now: " + score);
            System.out.println ("The correct answer was 'b) latte'.");
            System.out.println ("\nTry another question.");
        }

        else
        {
            JOptionPane.showMessageDialog (null, "That's NOT EVEN AN OPTION!", "Alert",
                    JOptionPane.ERROR_MESSAGE);
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was 'b) latte'.");
            System.out.println ("\nTry another question.");
        }

 


        char ans4 = IBIO.inputChar ("\n___ is the largest Starbucks cup size. \na)\ttall\t\nb)\tgrandisimo\nc)\tventi\nd)\tlarge\n");
        if (ans4 == ('c') || ans4 == ('C'))
        {
            score += 4;
            System.out.println ("\nThat's right. \nYou gained 4 point. Your score is now: " + score);
        }

        else if (ans4 == ('b') || ans4 == ('B'))
        {
            score += 3;
            System.out.println ("\nThat's wrong. \nYou still gained 3 points because you answer was close. Your score is now: " + score);
            System.out.println ("The correct answer was 'c) venti'.");
            System.out.println ("\nTry another question.");
        }

        else if (ans4 == ('a') || ans4 == ('A'))
        {
            score += 2;
            System.out.println ("\nThat's wrong. \nYou still gained 2 points because you answer wasn't absurd. Your score is now: " + score);
            System.out.println ("The correct answer was 'c) venti'.");
            System.out.println ("\nTry another question.");
        }

        else if (ans4 == ('d') || ans4 == ('D'))
        {
            score += 1;
            System.out.println ("\nThat's wrong. \nYou still gained 1 points because you answer was an option. Your score is now: " + score);
            System.out.println ("The correct answer was 'c) venti'.");
            System.out.println ("\nTry another question.");
        }

        else
        {
            JOptionPane.showMessageDialog (null, "That's NOT EVEN AN OPTION!", "Alert",
                    JOptionPane.ERROR_MESSAGE);
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was 'c) venti'.");
            System.out.println ("\nTry another question.");
        }

 

        char ans5 = IBIO.inputChar ("\n____ is the largest coffee producer in the world today. \na)\tBrazil\nb)\tColombia\nc)\tIndia\nd)\tEthiopia\n");
        if (ans5 == ('a') || ans5 == ('A'))
        {
            score += 4;
            System.out.println ("\nThat's right. \nYou gained 4 point. Your total score for hot drinks is: " + score);
        }

        else if (ans5 == ('b') || ans5 == ('B'))
        {
            score += 3;
            System.out.println ("\nThat's wrong. \nYou still gained 3 points because you answer was close. Your score is now: " + score);
            System.out.println ("The correct answer was 'a) Brazil'.");
            System.out.println ("\nTry another question.");
        }

        else if (ans5 == ('d') || ans5 == ('D'))
        {
            score += 2;
            System.out.println ("\nThat's wrong. \nYou still gained 2 points because you answer wasn't absurd. Your score is now: " + score);
            System.out.println ("The correct answer was 'a) Brazil'.");
            System.out.println ("\nTry another question.");
        }

        else if (ans5 == ('c') || ans5 == ('C'))
        {
            score += 1;
            System.out.println ("\nThat's wrong. \nYou still gained 1 points because you answer was an option. Your score is now: " + score);
            System.out.println ("The correct answer was 'a) Brazil'.");
            System.out.println ("\nTry another question.");
        }

        else
        {
            JOptionPane.showMessageDialog (null, "That's NOT EVEN AN OPTION!", "Alert",
                    JOptionPane.ERROR_MESSAGE);
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was 'a) Brazil'.");
            System.out.println ("\nTry another section.");
        }


        //prints prompts according to score
        if (score > 16)
            printSlow ("\nYou did very well!");
        if (score > 10)
        {
            printSlow ("\nYou have gained enough points to receive a hot drink!");
            //prints picture of coffee
            hotDrinkpic ();
        }


        else
            System.out.println ("\nSorry, you have not gained enough points to receive a hot drink! >_<\n\n\n\n");
        pause ();
        return score;

    }


