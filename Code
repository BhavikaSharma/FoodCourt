    public void hotDrinkName ()
    {
        System.out.println ("\t     .'(     .-./(  .-,.-.,-.      )\\.-.     /`-.  .'(   )\\  )\\      .'(    )\\.--.  .`(   ");
        System.out.println ("\t ,') \\  )  ,'     ) ) ,, ,. (    ,'     )  ,' _  \\ \\  ) (  \\, /   ,')\\  )  (   ._.' \\_ )  ");
        System.out.println ("\t(  '-' (  (  .-, (  \\( |(  )/   (  .-, (  (  '-' ( ) (   ) \\ (   (  '/ /    `-.`.     )\\  ");
        System.out.println ("\t ) .-.  )  ) '._\\ )    ) \\       ) '._\\ )  ) ,_ .' \\  ) ( ( \\ \\   )   (    ,_ (  \\    \\(  ");
        System.out.println ("\t(  ,  ) \\ (  ,   (     \\ (      (  ,   (  (  ' ) \\  ) \\  `.)/  ) (  .\\ \\  (  '.)  )    _  ");
        System.out.println ("\t )/    )/  )/ ._.'      )/       )/ ._.'   )/   )/   )/     '.(   )/  )/   '._,_.'    `.");
    }
}

