    //pauses for effect
    //written as a method to avoid repeating code
    public void pause ()
    {
        try
        {
            Thread.sleep (1000);
        }
        catch (InterruptedException m)
        {
            ;
        }
    }


    //continues introduction, more specifically, gives instructions
    public void Instructions ()
    {
        //Show a warning dialog with the options OK, CANCEL, title 'Warning',
        // and message 'Click OK to continue':

        Object[] options = {"What Happened?"};
        JOptionPane.showOptionDialog (null, "OH NO!", "Warning",
                JOptionPane.DEFAULT_OPTION, JOptionPane.WARNING_MESSAGE,
                null, options, options [0]);

        System.out.println ("\nThe snackbars in the food court have just closed. \nThe cashiers said they will open up for you if you answer some questions...");
        pause ();
        //picture of mall from http://es.wikipedia.org/wiki/Archivo:Southlands_Boulevarde_Food_Court.jpg
        JOptionPane.showMessageDialog (null, createImageIcon ("foodcourt.jpg"), "Food Court", JOptionPane.INFORMATION_MESSAGE);
        //confirms if user wants to play
        char hungry = IBIO.inputChar ("Do you still want food? (y/n) ");
        if (hungry == 'y' || hungry == 'Y')
        {
            option ();
        }

        else if (hungry == 'n' || hungry == 'N')
        {
            System.out.println ("\nOkay, have fun shopping!");
            //prints 'Goodbye' in a dialogue box
            JOptionPane.showMessageDialog (null, "\tG O O D B Y E !!!", "Bye!",
                    JOptionPane.INFORMATION_MESSAGE);
        }
        else
        {
            //Show an error dialog that displays the message, 'Error':
            JOptionPane.showMessageDialog (null, "Error", "Error",
                    JOptionPane.ERROR_MESSAGE);
            System.out.println ("\nSorry that's not a choice.");
            hungry = IBIO.inputChar ("Do you want food? (y/n) ");
            if (hungry == 'y' || hungry == 'Y')
                option ();
            else
                System.out.println ("\nOkay, have fun shopping!");
            JOptionPane.showMessageDialog (null, "\tG O O D B Y E !!!", "Bye!",
                    JOptionPane.INFORMATION_MESSAGE);

        }
    }


    //asks what food (method) user wants to go to
    //adds up points from each method when user is done
    public void option ()
    {
        int option = 1;
        //continues asking user question until answer is 9 (done)
        while (option != 9)
        {
            //from http://www.chris.com/ascii/index.php?art=objects/books (editted)
            System.out.println ("\nWhat do you want to eat?\nThe points for each individual section will be calculated seperately.\nThey will be added up at the end when you are finished eating.\nDon't pick an option twice!");
            pause ();
            System.out.println ("");
            System.out.println ("\t      _____________________________________________________");
            System.out.println ("\t  .-/|         ~~**~~         \\ /            ~~**~~        |\\-. ");
            System.out.println ("\t  ||||                         :                           ||||");
            System.out.println ("\t  ||||                         :                           ||||");
            System.out.println ("\t  |||| ,__ __                  :        1.   Burger        ||||");
            System.out.println ("\t  ||||/|  |  |                 :        2.   Sandwich      ||||");
            System.out.println ("\t  |||| |  |  |   _    _        :        3.   Fries         ||||");
            System.out.println ("\t  |||| |  |  |  |/  |/ | |   | :        4.   Cold Drink    ||||");
            System.out.println ("\t  |||| |  |  |  |__ |  |  \\_/|_:        5.   Hot Drink     ||||");
            System.out.println ("\t  ||||                         :        6.   Dessert       ||||");
            System.out.println ("\t  ||||                         :        7.   Pizza         ||||");
            System.out.println ("\t  ||||                         :        8.   Soup          ||||");
            System.out.println ("\t  ||||                         :        9.   Done          ||||");
            System.out.println ("\t  ||||                         :                           ||||");
            System.out.println ("\t  ||||                         :                           ||||");
            System.out.println ("\t  ||||________________________ : __________________________||||");
            System.out.println ("\t  ||/=========================\\:/===========================\\||");
            System.out.println ("\t  `--------------------------~___~--------------------------''");


            option = IBIO.inputInt ("\nEnter a number between 1 and 9 >>\n");
            pause ();
            switch (option)
            {
                case 1:
                    burgerpt = burger (points);
                    points += burgerpt;
                    break;
                case 2:
                    sandwichpt = sandwich (points);
                    points += sandwichpt;
                    break;
                case 3:
                    friespt = fries (points);
                    points += friespt;
                    break;
                case 4:
                    coldDrinkpt = coldDrink (points);
                    points += coldDrinkpt;
                    break;
                case 5:
                    hotDrinkpt = hotDrink (points);
                    points += hotDrinkpt;
                    break;
                case 6:
                    dessertpt = dessert (points);
                    points += dessertpt;
                    break;
                case 7:
                    points += pizza (points);
                    break;
                case 8:
                    points += soup (points);
                    break;

                    //ends loop
                case 9:
                    //prints out prompt according to points
                    if (burgerpt > 6)
                        System.out.println ("You received a burger.");
                    if (sandwichpt (points) > 6)
                        System.out.println ("You received a sandwich.");
                    if (fries (points) > 6)
                        System.out.println ("You received fries.");
                    if (coldDrink (points) > 6)
                        System.out.println ("You received a cold drink.");
                    if (hotDrink (points) > 10)
                        System.out.println ("You received a hot drink.");
                    if (dessert (points) > 6)
                        System.out.println ("You received a dessert.");
                    if (pizza (points) > 5)
                        System.out.println ("You received a slice of pizza.");
                    if (soup (points) > 2)
                        System.out.println ("You received soup.");

                    System.out.println ("\nYou have a total of " + points + " points.");
                    if (points > 40)
                        System.out.println ("\nYOU PASSED WITH FLYING COLOURS!!!");
                    else if (points > 10)
                        System.out.println ("\nYOU NEED TO WORK HARDER!");
                    else if (points > 0)
                        System.out.println ("\nYOU BARELY RECEIVED A POSITIVE SCORE!");
                    else
                        System.out.println ("\nI DIDN'T KNOW ANYONE COULD HAVE A SCORE THIS TERRIBLE!!!");

                    //asks if user wants to play again in a dialog box (pull down menu)
                    String[] again = {"Play Again", "End Game", };
                    String selectedValue = (String) JOptionPane.showInputDialog (null,
                            "You have finished playing!", "Again?", JOptionPane.INFORMATION_MESSAGE, null,
                            again, again [0]);

                    if (again.equals ("Play Again"))
                    {
                        printSlow ("\nI'm 'relish'-ing the fact that you liked my game!\nYou should try some more 'pi'!");
                        Introduction ();
                    }

                    //ends game
                    else
                    {
                        printSlow ("\nOkay, have fun shopping!\nI 'cannoli' imagine what you're going to buy...");
                        printSlow ("I'm glad you're not hungry anymore,\nI already 'mustard' up all the strength that I had so I could to feed you today!");
                        //prints 'Goodbye' in a dialogue box
                        JOptionPane.showMessageDialog (null, "\tG O O D B Y E !!!", "Bye!",
                                JOptionPane.INFORMATION_MESSAGE);
                        break;
                    }
                    break;

            }
        }
    }

 

    //tells user to choose a character
    public void character ()
    {

        int option = IBIO.inputInt ("\nEnter your choice character>>\n");
        System.out.print ("You picked: ");
        pause ();
        //opens dialogue box with picture of character corrsponding to what user picks
        //all pictures from http://www.zcool.com.cn/gfx/ZMjE4NTUy.html (editted)
        switch (option)
        {
            case 1:
                JOptionPane.showMessageDialog (null, createImageIcon ("jake.jpg"), "Jake", JOptionPane.INFORMATION_MESSAGE);
                break;
            case 2:
                JOptionPane.showMessageDialog (null, createImageIcon ("sarah.jpg"), "Sarah", JOptionPane.INFORMATION_MESSAGE);
                break;
            case 3:
                JOptionPane.showMessageDialog (null, createImageIcon ("aaron.jpg"), "Aaron", JOptionPane.INFORMATION_MESSAGE);
                break;
            case 4:
                JOptionPane.showMessageDialog (null, createImageIcon ("jessica.jpg"), "Jessica", JOptionPane.INFORMATION_MESSAGE);
                break;
            case 5:
                JOptionPane.showMessageDialog (null, createImageIcon ("derek.jpg"), "Derek", JOptionPane.INFORMATION_MESSAGE);
                break;
            case 6:
                JOptionPane.showMessageDialog (null, createImageIcon ("brooke.jpg"), "Brooke", JOptionPane.INFORMATION_MESSAGE);
                break;
        }
    }


    //makes letters print slowly (pauses after each letter)
    public void printSlow (String s)
    {
        for (int i = 0 ; i < s.length () ; i++)
        {
            System.out.print ("" + s.charAt (i));
            //sleep for a bit after printing a letter
            try
            {
                Thread.sleep (100);
            }
            catch (InterruptedException m)
            {
                ;
            }
        }


        System.out.println ();
    }


    //user picked cold drinks from menu (option method)
    //asks questions about cold drinks
    //asks for a word (String variable)
    //keeps track of score for this section (therefore score is reset) and returns it at the end (as an integer)
    public int coldDrink (int score)
    {
        score = 0;

        System.out.println ("\n\t\t\t***You picked***");
        coldDrinksName ();

        System.out.println ("\n\nHopefully, you're going to eat food as well as drink...\nHere's a 'pop' quiz!\n");

        String ans1 = IBIO.inputString ("What colour is most predominant on a coke can?\n");
        if (ans1.equalsIgnoreCase ("red"))
        {
            score++;
            System.out.println ("\nThat's right. \nYou gained a point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was red.");
        }


        String ans2 = IBIO.inputString ("\nThe second question is pretty easy. \nUnscramble this drink: unmtinao ewd\n");
        if (ans2.equalsIgnoreCase ("mountain dew"))
        {
            score += 2;
            System.out.println ("\nThat's right. \nYou gained 2 point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was mountain dew");
        }


        String ans3 = IBIO.inputString ("\nThis question is a little harder.\nComplete the sentence: Could have had a...\n");
        if (ans3.equalsIgnoreCase ("v8"))
        {
            score += 3;
            System.out.println ("\nThat's right. \nYou gained 3 point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was V8.");
        }


        String ans4 = IBIO.inputString ("\nYou're almost finished.\nI am a drink served during Christmas time, generally served cold.\n");
        if (ans4.equalsIgnoreCase ("eggnog") || ans4.equalsIgnoreCase ("egg nog"))
        {
            score += 4;
            System.out.println ("\nThat's right. \nYou gained 4 point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was eggnog.");
        }


        String ans5 = IBIO.inputString ("\nYour thirst is going to soon be quenched.\nWhat am I? A drink? A medic? An archelogist? A spice?\n");
        if (ans5.equalsIgnoreCase ("Dr. Pepper") || ans5.equalsIgnoreCase ("dr pepper"))
        {
            score += 5;
            System.out.println ("\nThat's right. \nYou gained 5 point. Your total score for cold drinks is: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another section.");
            score--;
            System.out.println ("You lost a point. Your total score for cold drinks is: " + score);
            System.out.println ("The correct answer was Dr. Pepper.");
        }

        //prints prompts according to score
        if (score > 11)
            printSlow ("\nYou did very well!");
        if (score > 6)
        {
            printSlow ("\nYou have gained enough points to receive a cold drink!");
            //prints picture of pop
            coldDrinkpic ();
        }


        else
            System.out.println ("\nSorry, you have not gained enough points to receive a cold drink! >_<\n\n\n\n");
        pause ();

        return score;

    }


    //user picked fries from menu (option method)
    //asks questions about fries
    //asks for a true (t) or false (f) (char variable)
    //keeps track of score for this section (therefore score is reset) and returns it at the end (as an integer)
    public int fries (int score)
    {
        score = 0;

        System.out.println ("\n\t\t\t***You picked***");
        friesName ();

        System.out.println ("\n\nThis is not going to be enough to fill you up...\nTry not to get 'fried' in this quiz!\n");

        System.out.println ("\nThe following are all true or false questions\nAnswer 't' for true and 'f' for false");

        char ans1 = IBIO.inputChar ("\nPoutine is fries with ketchcup and cheese\n");
        while (ans1 != 'f' && ans1 != 't')
        {
            {
                //Show an error dialog that displays the message, 'That's NOT EVEN AN OPTION!':
                JOptionPane.showMessageDialog (null, "That's NOT EVEN AN OPTION!", "Alert",
                        JOptionPane.ERROR_MESSAGE);
                score--;
                System.out.println ("You lost a point. Your score is now: " + score);
            }
            ans1 = IBIO.inputChar ("\nPoutine is fries with ketchcup and cheese\n");
        }
        if (ans1 == ('f'))
        {
            score++;
            System.out.println ("\nThat's right. \nYou gained a point. Your score is now: " + score);
        }

        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was 'f', they are fries with gravy and cheese.");
        }


        char ans2 = IBIO.inputChar ("\nFries are called 'crisps' in England\n");
        while (ans2 != 'f' && ans2 != 't')
        {
            {
                JOptionPane.showMessageDialog (null, "That's NOT EVEN AN OPTION!", "Alert",
                        JOptionPane.ERROR_MESSAGE);
                score--;
                System.out.println ("You lost a point. Your score is now: " + score);
            }
            ans2 = IBIO.inputChar ("\nFries are called 'crisps' in England\n");
        }

        if (ans2 == ('f'))
        {
            score += 2;
            System.out.println ("\nThat's right. \nYou gained 2 point. Your score is now: " + score);
        }

        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was 'f', they are called 'chips'.");
        }


        char ans3 = IBIO.inputChar ("\nMcCain is a popular frozen fries brand.\n");
        while (ans3 != 'f' && ans3 != 't')
        {
            {
                JOptionPane.showMessageDialog (null, "That's NOT EVEN AN OPTION!", "Alert",
                        JOptionPane.ERROR_MESSAGE);
                score--;
                System.out.println ("You lost a point. Your score is now: " + score);
            }
            ans3 = IBIO.inputChar ("\nMcCain is a popular frozen fries brand.\n");
        }

        if (ans3 == ('t'))
        {
            score += 3;
            System.out.println ("\nThat's right. \nYou gained 3 point. Your score is now: " + score);
        }

        else
        {
            System.out.println ("\nThat's wrong, try another question.")
                ;
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was 't'.");
        }

        char ans4 = IBIO.inputChar ("\nFrench fries are from France.\n");
        while (ans4 != 'f' && ans4 != 't')
        {
            {
                JOptionPane.showMessageDialog (null, "That's NOT EVEN AN OPTION!", "Alert",
                        JOptionPane.ERROR_MESSAGE);
                score--;
                System.out.println ("You lost a point. Your score is now: " + score);
            }
            ans4 = IBIO.inputChar ("\nFrench fries are from France.\n");
        }

        if (ans4 == ('f'))
        {
            score += 4;
            System.out.println ("\nThat's right. \nYou gained 4 point. Your score is now: " + score);
        }

        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was 'f', they are from Belgium.");
        }

        char ans5 = IBIO.inputChar ("\nSpiralled fries are known as 'tornado fries'.\n");
        while (ans5 != 'f' && ans5 != 't')
        {
            {
                JOptionPane.showMessageDialog (null, "That's NOT EVEN AN OPTION!", "Alert",
                        JOptionPane.ERROR_MESSAGE);
                score--;
                System.out.println ("You lost a point. Your score is now: " + score);
            }
            ans5 = IBIO.inputChar ("\nSpiralled fries are known as 'tornado fries'.\n");
        }
        if (ans5 == ('t'))
        {
            score += 5;
            System.out.println ("\nThat's right. \nYou gained 5 point. Your total score for fries is: " + score);
        }

        else
        {
            System.out.println ("\nThat's wrong, try another section.");
            score--;
            System.out.println ("You lost a point. Your total score for fries is: " + score);
            System.out.println ("The correct answer was 't'.");
        }


        //prints prompts according to score
        if (score > 11)
            printSlow ("\nYou did very well!");
        if (score > 6)
        {
            printSlow ("\nYou have gained enough points to receive fries!");
            //prints picture of fries
            friespic ();
        }


        else
            System.out.println ("\nSorry, you have not gained enough points to receive fries! >_<\n\n\n\n");
        pause ();
        return score;

    }


    //user picked sandwich from menu (option method)
    //asks math questions
    //asks for an integer (int variable)
    //keeps track of score for this section (therefore score is reset) and returns it at the end (as an integer)
    public int sandwich (int score)
    {
        score = 0;

        System.out.println ("\n\t\t\t\t***You picked***");
        sandwichName ();

        System.out.println ("\n\nDisregard all decimal places when writing your answers.\nI hope the food tastes better than the 'sand which' are served in in other stores...\n");

        int ans1 = IBIO.inputInt ("(5+5+5+5)/4\n");
        if (ans1 == (5 + 5 + 5 + 5) / 4)
        {
            score++;
            System.out.println ("\nThat's right. \nYou gained a point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was 5");
        }


        int ans2 = IBIO.inputInt ("\n(7*8)-11+5\n");
        if (ans2 == ((7 * 8) - 11 + 5))
        {
            score += 2;
            System.out.println ("\nThat's right. \nYou gained 2 point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was 50");
        }

        //uses random integers, therefore changing up the question
        int random = (int) (Math.random () * 10) + 0;
        int ans3 = IBIO.inputInt ("\nYou should know this from learning about binary numbers.\nWhat is 2^" + random + "?\n");

        if (ans3 == (Math.pow (2, random)))
        {
            score += 3;
            System.out.println ("\nThat's right. \nYou gained 3 point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was " + (Math.pow (2, random)));
        }

        random = (int) (Math.random () * 100) + 1;
        int ans4 = IBIO.inputInt ("\nWhat remanider do you get when you divide 100 by " + random + "?\n");
        if (ans4 == 100 % random)
        {
            score += 4;
            System.out.println ("\nThat's right. \nYou gained 4 point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was " + (100 % random));
        }


        random = (int) (Math.random () * 10) + 3;
        int ans5 = IBIO.inputInt ("\nWhat is 7^" + random + "+7?\n");
        if (ans5 == (Math.pow (7, random)) + 7)
        {
            score += 5;
            System.out.println ("\nThat's right. \nYou gained 5 point. Your total score for sandwich is: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another section.");
            score--;
            System.out.println ("You lost a point. Your total score for sandwich is: " + score);
            System.out.println ("The correct answer was " + (Math.pow (7, random) + 7));
        }

        //prints prompts according to score
        if (score > 11)
            printSlow ("\nYou did very well!");
        if (score > 6)
        {
            printSlow ("\nYou have gained enough points to receive a sandwich!");
            //prints picture of a sandwich
            sandwichpic ();
        }


        else
            System.out.println ("\nSorry, you have not gained enough points to receive a sandwich! >_<\n\n\n\n");
        pause ();
        return score;
    }


    //rounds a number (num) to a specific number (digit) of decimal places
    public double round (double num, int digit)
    {
        double num2 = num * Math.pow (10, digit);
        double num3 = (num2 - ((int) num2)) * 10;
        num2 = ((int) num2);
        if (num3 >= 5)
            num2++;
        return num2 /= Math.pow (10, digit);
    }

 

    //user picked burger from menu (option method)
    //asks math questions
    //asks for a 2 decimal places (double variable)
    //keeps track of score for this section (therefore score is reset) and returns it at the end (as an integer)
    public int burger (int score)
    {
        score = 0;

        System.out.println ("\n\t\t\t\t***You picked***");
        burgerName ();

        System.out.println ("\n\nAre you ready for harder questions?\nRound all answers to 2 decimal places when writing your answers.\nWe sell hamburgers, chicken burgers and cheese burgers (all the regulars), and for the vegetarians we have 'de-calf'...\n\n");

        int digit = 2;

        double answer = ((Math.pow (25, 0.5)));
        double ans1 = IBIO.inputDouble ("What is 25^(1/2)?\n");
        double r = round (answer, digit);
        if (ans1 == r)
        {
            score++;
            System.out.println ("\nThat's right. \nYou gained a point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was " + r);
        }

        //uses a random decimal number, therefore changing up the question each time
        double random = (double) (Math.random () * 10) + 0;
        //rounds random decimal number to two decimal places
        double rRandom = round (random, digit);
        answer = ((Math.pow (rRandom, 3)));
        double ans2 = IBIO.inputDouble ("\nWhat is the volume of a cube with the width of " + rRandom + "?\n");
        r = round (answer, digit);
        if (ans2 == r)
        {
            score += 2;
            System.out.println ("\nThat's right. \nYou gained 2 point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was " + r);
        }

        //uses a random integer, therefore changing up the question each time
        int random1 = (int) (Math.random () * 10) + 0;
        answer = ((Math.PI) * (Math.pow ((random1 * 0.5), 2)));
        double ans3 = IBIO.inputDouble ("\nWhat is the area of a circle with the diameter of " + random1 + " cm?\n");
        r = round (answer, digit);

        if (ans3 == r)
        {
            score += 3;
            System.out.println ("\nThat's right. \nYou gained 3 point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was " + r);
        }


        random = (double) (Math.random () * 10) + 0;
        double height = (double) (Math.random () * 10) + 0;
        rRandom = round (random, digit);
        double hRandom = round (height, digit);
        answer = ((Math.PI) * (Math.pow ((rRandom), 2)) * (hRandom)) / 3;
        double ans4 = IBIO.inputDouble ("\nWhat is the volume of a cone with the radius of " + rRandom + " and a height of " + hRandom + "?\n");
        r = round (answer, digit);
        if (ans4 == r)
        {
            score += 4;
            System.out.println ("\nThat's right. \nYou gained 4 point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
            System.out.println ("The correct answer was " + r);
        }


        printSlow ("\nYou must answer this question correctly to proceed.");
        random1 = (int) (Math.random () * 1000) + 101;
        int random2 = (int) (Math.random () * 100) + 101;
        answer = ((Math.pow (random1, 0.5)) + ((Math.pow (random2, 0.5))));
        double ans5 = IBIO.inputDouble ("\nWhat is " + random1 + "^(1/2) + " + random2 + "^(1/2)?\n");
        r = round (answer, digit);
        if (ans5 == r)
        {
            score += 5;
            System.out.println ("\nThat's right. \nYou gained 5 point. Your score is now: " + score);
        }

 

        else
        {
            System.out.println ("\nThat's wrong, try again");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);

            //doesn't allow user to continue without entering the correct answer (r variable)
            while (ans5 != r)
            {
                ans5 = IBIO.inputDouble ("\nWhat is " + random1 + "^(1/2) + " + random2 + "^(1/2)?\n");
                if (ans5 == r)
                {
                    score += 5;
                    System.out.println ("\nThat's right. \nYou gained 5 point. Your total score for burger is: " + score);
                }

 

                else
                {
                    System.out.println ("\nThat's wrong, try again");
                    score--;
                    System.out.println ("You lost a point. Your score is now: " + score);
                }
            }
        }

        //prints prompts according to score
        if (score > 11)
            printSlow ("\nYou did very well!");
        if (score > 6)
        {
            printSlow ("\nYou have gained enough points to receive a burger!");
            //prints picture of a burger
            burgerpic ();
        }


        else
            System.out.println ("\nSorry, you have not gained enough points to receive a burger! >_<\n\n\n\n");
        pause ();
        return score;
    }


    //checks if a number is a prime number
    boolean isPrime (int n)
    {
        for (int i = 2 ; i < n ; i++)
        {
            if (n % i == 0)
                return false;
        }
        return true;
    }


    public boolean ValidDessert (double ans4, double random2)
    {
        if ((ans4 == (random2 / 4)))
            return true;
        else if (ans4 == (random2 / 8))
            return true;
        else if (ans4 == (random2 / 60))
            return true;
        else if ((ans4 == 0) && (random2 == 0))
            return true;
        else
            return false;

    }


    //user picked dessert from menu (option method)
    //asks math questions
    //asks for integers and decimals (int and double variables)
    //keeps track of score for this section (therefore score is reset) and returns it at the end (as an integer)
    public int dessert (int score)
    {
        score = 0;

        System.out.println ("\n\t\t\t\t***You picked***");
        dessertName ();

        System.out.println ("\n\nThis round is a 'piece of cake'!\n\n");

        int digit = 2;


        int ans1 = IBIO.inputInt ("What is a integer in between 1 and 1000000 and is divisible by 2?\n");
        if (ans1 >= 1 && ans1 <= 1000000 && (ans1 % 2 == 0))
        {
            score++;
            System.out.println ("\nThat's right. \nYou gained a point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
        }


        int random1 = (int) (Math.random () * 10) + 1;
        double ans2 = IBIO.inputDouble ("\nJennie wants to buy chocolate bars. If 1 costs $0.67 in one store and $0.93 in another,\nhow much money will Jennie need to buy " + random1 + " bars (you can answer for either store)?\n");
        if (ans2 == random1 * 0.67 || ans2 == random1 * 0.93)
        {
            score += 2;
            System.out.println ("\nThat's right. \nYou gained 2 point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
        }


        int ans3 = IBIO.inputInt ("\nWhat is an odd integer below 100, an even integer over 1000 or a prime integer between 500 and 600?\n");
        boolean p = isPrime (ans3);
        if ((ans3 < 100 && ans3 % 2 != 0) || (ans3 > 1000 && ans3 % 2 != 1) || (p == true && ans3 > 500 && ans3 < 600))
        {
            score += 3;
            System.out.println ("\nThat's right. \nYou gained 3 point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
        }

 

        random1 = (int) (Math.random () * 10) + 0;
        double random2 = random1;
        double ans4 = IBIO.inputDouble ("\nParker can walk 4 km, bike 8 km or drive 60 km in one hour.\nHow long does it take him to perform any of these tasks for " + random1 + " kilometers (in hrs)?\n");
        if (ValidDessert (ans4, random2))
        {
            score += 4;
            System.out.println ("\nThat's right. \nYou gained 4 point. Your score is now: " + score);
        }


        else
        {
            System.out.println ("\nThat's wrong, try another question.");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);
        }

 

 

        printSlow ("\nYou must answer this question correctly to proceed.");
        int ans5 = IBIO.inputInt ("\nEnter a root of this question:\ny=x^2+5x+6\n");
        if (ans5 == 2 || ans5 == 3)
        {
            score += 5;
            System.out.println ("\nThat's right. \nYou gained 5 point. Your score is now: " + score);
        }

 

        else
        {
            System.out.println ("\nThat's wrong, try again");
            score--;
            System.out.println ("You lost a point. Your score is now: " + score);

            //doesn't allow user to continue without entering the correct answer (2 or 3)
            while (ans5 != 2 && ans5 != 3)
            {
                ans5 = IBIO.inputInt ("\nEnter a root of this question:\ny=x^2+5x+6\n");
                if (ans5 == 2 || ans5 == 3)
                {
                    score += 5;
                    System.out.println ("\nThat's right. \nYou gained 5 point. Your total score for dessert is: " + score);
                }

 

                else
                {
                    System.out.println ("\nThat's wrong, try again");
                    score--;
                    System.out.println ("You lost a point. Your score is now: " + score);
                }
            }

        }

        //prints prompts according to score
        if (score > 11)
            printSlow ("\nYou did very well!");
        if (score > 6)
        {
            printSlow ("\nYou have gained enough points to receive a dessert!");
            //prints picture of ice cream
            dessertpic ();
        }


        else
            System.out.println ("\nSorry, you have not gained enough points to receive a dessert! >_<\n\n\n\n");
        pause ();
        return score;

    }


