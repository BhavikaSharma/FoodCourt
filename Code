//necessary for extra features
import javax.swing.*;
import java.util.*;
import java.text.*;


public class foodCourt
{
    public static void main (String args[])
    {
        new foodCourt ();
    }


    int points = 0;
    int burgerpt = 0;
    int sandwichpt = 0;
    int dessertpt = 0;
    int coldDrinkpt = 0;
    int hotDrinkpt = 0;
    int pizzapt = 0;
    int souppt = 0;
    int friespt = 0;

    public foodCourt ()
    {
        Introduction ();
    }


    //creates a table of numbers that will be used for questions in soup method
    //number multiply by 5 within a column and 4 within a row
    public void multiply ()
    {

        for (int i = 1 ; i <= 15000 ; i *= 5)
        {

            int counter = 1;
            for (counter = 1 ; counter <= 15000 ; counter *= 4)
            {
                int z = (i * counter);
                System.out.print (z + "\t");

            }
            System.out.println ("");
            counter++;
        }
    }


    //necessary for adding pictures in dialogue boxes
    protected static ImageIcon createImageIcon (String path)
    {

        java.net.URL imgURL = foodCourt.class.getResource (path);
        if (imgURL != null)
        {
            return new ImageIcon (imgURL);
        }


        else
        {
            System.err.println ("Couldn't find file: " + path);
            return null;
        }
    }


